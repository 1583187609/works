#bottom-line{
  position: relative;
  width: 100%;
  height: 60rpx;
  margin: 30rpx 0;
}
#bottom-line view{
  height: 1px;
  width: calc(100% - 30rpx);
  background: #555555;
}
#bottom-line text{
  color: #777777;
  position: absolute;
  left: 50%;
  padding: 0 20rpx;
  font-size: 30rpx;
  letter-spacing: 1rpx;
  transform: translate(-50%,0);
  border-radius: 10rpx;
  background: #333333;
}